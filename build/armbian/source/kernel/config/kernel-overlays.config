# kernel-overlays.config - Дополнительные настройки для ядра

# Добавление поддержки специфичных устройств
CONFIG_USB_SERIAL_CP210X=m  # Драйвер для USB-устройств CP210X
CONFIG_BT_BCM=m             # Поддержка Bluetooth чипов Broadcom

# Дополнительные параметры ядра для оптимизации
CONFIG_PREEMPT_RT_FULL=y    # Реализация реального времени для низкой задержки

# Экспериментальные драйверы
CONFIG_EXPERIMENTAL_FEATURE=y  # Включение экспериментальной функции
